{
  "name": "gatsby-source-wikipedia",
  "version": "4.19.0-next.1",
  "description": "Gatsby source plugin for pulling articles from Wikipedia",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "wikipedia"
  ],
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-wikipedia#readme",
  "peerDependencies": {
    "gatsby": "^4.0.0-next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-source-wikipedia"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "bluebird": "^3.7.2",
    "node-fetch": "^2.6.7",
    "query-string": "^6.14.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "babel-preset-gatsby-package": "^2.19.0-next.1",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
