    "lodash-webpack-plugin": "^0.11.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "babel-preset-gatsby-package": "^2.19.0-next.1",
    "cross-env": "^7.0.3"
